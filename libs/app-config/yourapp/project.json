{
    "name": "app-config-yourapp",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/app-config/yourapp/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/app-config/yourapp",
                "main": "libs/app-config/yourapp/src/index.ts",
                "tsConfig": "libs/app-config/yourapp/tsconfig.lib.json",
                "assets": ["libs/app-config/yourapp/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/app-config/yourapp/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/app-config/yourapp"],
            "options": {
                "jestConfig": "libs/app-config/yourapp/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
